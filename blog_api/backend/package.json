{
  "name": "blog-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "nodemon": "nodemon --swc ./bin/www.ts",
    "watch": "ts-node --swc ./src/bin/www.ts",
    "watch_tsc": "tsc -w & nodemon ./dist/src/bin/www",
    "dev": "DEBUG=blog-api:*,app,controller:* npm run nodemon",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node ./dist/bin/www",
    "tsnode": "ts-node --swc script.ts",
    "build": "rimraf dist && npx tsc"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.0.1",
    "http-errors": "~1.6.3",
    "mongoose": "^7.5.2",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.82",
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.14",
    "@types/debug": "^4.1.9",
    "@types/express": "^4.17.17",
    "@types/express-validator": "^3.0.0",
    "@types/http-errors": "^2.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
