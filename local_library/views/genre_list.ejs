<!DOCTYPE html>
<html>
<%- include('partials/head', {title: title}); %>

<body>
  <main>
    <%- include('partials/sidebar'); %>
    <div class="content">
      <h1>
        <%= title %>
      </h1>
      <% genre_list.forEach(genre=> { %>
      <ul>
        <li>
          <a href="<%= genre.url %>">
            <%= genre.name %>
          </a>
        </li>
      </ul>
      <%}) %>
      <% if (!genre_list) { %>
      <h2>There are no genres</h2>
      <% } %>
    </div>
  </main>
</body>

</html>